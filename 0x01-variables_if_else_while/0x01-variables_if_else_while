#include <stdlib.h>
#include <time.h>
#include <stdio.h>

/**
 * main - entry point

 * Description: a

 * Return: 0
*/

int main(void)

{
	int n, digit;

	srand(time(0));
	n = rand() - RAND_MAX / 2;
	digit = n % 10
	if (digit > 5)
		printf("");
	else if (digit == 0)
		printf("");
	else if (digit < 6 && digit != O)
		printf("Last digit of %i is %i and is less than 6 and not 0\n", n,f digit);
	return (0);
}
